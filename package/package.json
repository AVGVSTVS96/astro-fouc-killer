{
  "name": "astro-fouc-killer",
  "version": "0.3.0",
  "description": "Eliminate Dark Mode FOUC (Flash Of Unstyled Content) in Astro. Works with class based dark modes set in local storage and supports system preference.",
  "author": {
    "email": "bassim@shahidy.com",
    "name": "Bassim Shahidy",
    "url": "https://bassimshahidy.com"
  },
  "license": "AGPL-3.0-only",
  "keywords": [
    "astro-integration",
    "astro-component",
    "withastro",
    "astro"
  ],
  "homepage": "https://github.com/avgvstvs96/astro-fouc-killer",
  "repository": "https://github.com/AVGVSTVS96/astro-fouc-killer",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "type": "module",
  "peerDependencies": {
    "astro": "^4.12.2"
  },
  "dependencies": {
    "astro-integration-kit": "^0.15.0"
  },
  "devDependencies": {
    "tsup": "^8.2.1"
  }
}
